<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.neu.ipco.entity">

	<class name="ActivityTemplate" table="activity_template">
		
		<id name="activityTemplateId" column="activity_template_id">
			<generator class="increment"/>
		</id>

		<property name="activityTemplateName" column="activity_template_name"/>

		<property name="createdTs" type="timestamp" column="created_ts"/>
		<property name="updatedTs" type="timestamp" column="updated_ts"/>
	</class>
</hibernate-mapping>
